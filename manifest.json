{
  "description": "Filter adult content users on soundcloud",
  "manifest_version": 2,
  "name": "sc-bot-filter",
  "version": "1.0",
  "homepage_url": "https://github.com/copoer/sc-bot-filter",
  "icons": {
    "48": "icon.png"
  },

  "permissions": [
    "webRequest", "webRequestBlocking", "https://api-v2.soundcloud.com/*", "https://soundcloud.com/*"
  ],

  "background": {
    "scripts": ["background.js"]
  }
}
